struct Point {
    x: felt
}

const GLOBAL = 42;
const UNUSED = 100; // Should warn: Unused variable 'UNUSED'

func test_struct_field_access() {
    let p = Point { x: 1 };
    let value = p.x; // OK: p is defined
    return();
}

func test_const_usage() {
    let value = GLOBAL; // OK: GLOBAL is defined
