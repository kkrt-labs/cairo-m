func test_simple_unused() {
    let unused = 42; // Should warn: Unused variable 'unused'
    let used = 24;
    return used;
}

func test_unused_parameter(unused_param: felt, used_param: felt) { // Should warn: Unused variable 'unused_param'
    return used_param;
}

func test_variables_used_in_expression() {
    let a = 1;
    let b = 2;
    return a + b; // Both a and b are used - no warnings
}
