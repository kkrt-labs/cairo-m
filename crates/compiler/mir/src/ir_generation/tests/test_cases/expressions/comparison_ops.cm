// Tests new comparison operations.
func test_less(a: felt, b: felt) -> felt {
    let c = a < b;
    return c;
}

func test_greater(a: felt, b: felt) -> felt {
    let c = a > b;
    return c;
}

func test_less_equal(a: felt, b: felt) -> felt {
    let c = a <= b;
    return c;
}

func test_greater_equal(a: felt, b: felt) -> felt {
    let c = a >= b;
    return c;
}

// Test comparison operators in conditional context
func test_comparison_in_if(x: felt, y: felt) -> felt {
    if (x < y) {
        return 1;
    } else if (x > y) {
        return 2;
    } else {
        return 0;
    }
}

// Test combined logical and comparison operators
func test_combined_operators(a: felt, b: felt, c: felt) -> felt {
    // Combine different comparison operators with logical operators
    if ((a < b) && (b > c)) {
        return 1;
    } else if ((a == b) || (b <= c)) {
        return 2;
    } else if ((a != b) && (a >= c)) {
        return 3;
    }
    
    return 0;
}