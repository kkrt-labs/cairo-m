fn powers() -> felt {
    let base = 3;
    let p2 = base * base;         // 3^2 = 9
    let p3 = p2 * base;           // 3^3 = 27
    let p4 = p3 * base;           // 3^4 = 81
    return p2 + p3 + p4;          // 9 + 27 + 81 = 117
}
