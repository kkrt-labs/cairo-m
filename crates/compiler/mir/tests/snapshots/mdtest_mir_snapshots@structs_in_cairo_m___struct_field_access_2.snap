fn calculate_area_u32() -> u32 {
    let rect = RectangleU32 { width: 5, height: 10 };
    if rect.width == 5 {
        rect.width = rect.width * 2;
    } else {
        rect.width = rect.height;
    }
    return rect.width * rect.height;
}
