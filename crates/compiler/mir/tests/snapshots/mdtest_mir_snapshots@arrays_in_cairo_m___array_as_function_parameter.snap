fn process_array(arr: [u32; 3], size: u32) -> u32 {
    let sum: u32 = 0;
    let i = 0u32;
    loop {
        if (i == size) {
            break;
        }
        sum = sum + arr[i];
        arr[i] = 0;
        i = i + 1;
    }
    return sum;
}

fn use_array_parameter() -> u32 {
    let my_array: [u32; 3] = [1, 2, 3];
    return process_array(my_array, 3u32);
}
