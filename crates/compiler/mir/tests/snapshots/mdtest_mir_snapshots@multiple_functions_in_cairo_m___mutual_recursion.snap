fn test_mutual_recursion() -> felt {
    let n = 5;
    return is_even(n) * 10 + is_odd(n);
}

fn is_even(n: felt) -> felt {
    if n == 0 {
        return 1;
    }
    return is_odd(n - 1);
}

fn is_odd(n: felt) -> felt {
    if n == 0 {
        return 0;
    }
    return is_even(n - 1);
}
