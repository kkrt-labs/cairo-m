    let max_u32: u32 = 4294967295;  // 2^32 - 1
    let overflow_add = max_u32 + 1;  // Wraps to 0
    let underflow_sub: u32 = 0;
    let underflow = underflow_sub - 1;  // Wraps to max u32
    return overflow_add + underflow;  // 0 + 4294967295
}
