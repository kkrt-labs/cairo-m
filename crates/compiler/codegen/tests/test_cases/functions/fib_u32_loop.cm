fn fibonacci_loop(n: u32) -> u32 {
    let a: u32 = 0;
    let b: u32 = 1;
    let i: u32 = 0;
    while (i != n) {
        let temp = a + b;
        a = b;
        b = temp;
        i = i + 1;
    }
    return a;
}

fn main() -> u32 {
    let n: u32 = 10;
    let result = fibonacci_loop(n);
    return result;
}
