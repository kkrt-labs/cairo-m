// Test case for variable-sized instructions with jumps
// This tests that jump offsets are calculated correctly based on physical memory layout

fn test_variable_jump(x: felt) -> felt {
    let a = x + 1;

    // This should generate a U32StoreAddFpImm instruction (size 5)
    // which takes 2 QM31 slots instead of 1
    let b = a + 0x10000;  // Large immediate that requires U32 instruction

    if (b == 0) {
        // Jump should account for the variable-sized instruction above
        return 1;
    }

    return b;
}

fn main() -> felt {
    return test_variable_jump(5);
}
