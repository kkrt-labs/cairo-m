fn pure_expression() -> felt {
    let x = 10;
    let y = 20;

    // All operations here are pure - no side effects
    let result = (x + y) * (x - y) / 2 + x * y;  // (10 + 20) * (10 - 20) / 2 + 10 * 20
                                                   // = 30 * (-10) / 2 + 200
                                                   // = -300 / 2 + 200
                                                   // = -150 + 200 = 50
    return result;
}
