fn test_precedence() -> felt {
    let a = 5;
    let b = 3;
    let c = 2;

    // Multiplication happens before addition: 5 + (3 * 2) = 11
    let result1 = a + b * c;

    // Parentheses override precedence: (5 + 3) * 2 = 16
    let result2 = (a + b) * c;

    // Complex precedence: 5 * 3 + 2 * 4 = 23
    let result3 = a * b + c * 4;

    return result1 + result2 + result3;  // 11 + 16 + 23 = 50
}
