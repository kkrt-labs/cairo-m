fn loop_with_escape() -> u32 {
    let counter = 0u32;

    loop {
        counter = counter + 1u32;

        // Multiple exit conditions
        if (counter > 1000u32) {
            break;  // Prevent actual infinite loop
        }

        if (counter == 10u32) {
            break;
        }
    }

    return counter;  // Should return 100
}
