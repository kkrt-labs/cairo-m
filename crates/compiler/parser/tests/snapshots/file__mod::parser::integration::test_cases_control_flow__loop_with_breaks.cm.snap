fn loop_control_flow() {
    // Break in different contexts
    loop {
        break;
    }

    while true {
        if (should_exit()) {
            break;
        }
    }

    for (let item = 0; item < collection_len; item = item + 1) {
        if (is_invalid(item)) {
            break;
        }
        process(item);
    }

    // Continue in different contexts
    let sum = 0;
    for (let i = 0; i < numbers_len; i = i + 1) {
        if (i != 0) {
            continue;
        }
        sum = sum + i;
    }

    // Multiple breaks and continues
    loop {
        let x = read_input();
        if (x == 0) {
            break;
        }
        if (x != 0) {
            continue;
        }
        process_positive(x);
    }
}

