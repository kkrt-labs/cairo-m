        use std::math::sqrt;
        use std::io::print;

        struct Point {
            x: felt,
            y: felt
        }

        fn distance(p1: Point, p2: Point) -> felt {
            let dx: felt = p1.x - p2.x;
            let dy: felt = p1.y - p2.y;
            return sqrt(dx * dx + dy * dy);
        }
    
