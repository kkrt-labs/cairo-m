        fn test_loops() {
            loop {
                let x = 1;
                if x == 1 {
                    break;
                }
            }

            let counter = 0;
            while counter != 10 {
                counter = counter + 1;
            }

            for (let i = 0; i < 10; i = i + 1) {
                let squared = i * i;
            }
        }
    
