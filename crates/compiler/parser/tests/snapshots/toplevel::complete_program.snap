        struct Vector {
            x: felt,
            y: felt
        }

        fn magnitude(v: Vector) -> felt {
            return (v.x * v.x + v.y * v.y);
        }

        fn rfib(n: felt) -> felt {
            if n == 0 {
                return 0;
            }
            if n == 1 {
                return 1;
            }
            return rfib(n - 1) + rfib(n - 2);
        }

        const TOP_LEVEL_CONST = 100;
        const TYPED_CONST: felt = 42;
    
