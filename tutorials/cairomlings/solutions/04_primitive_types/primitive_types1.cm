// Booleans (`bool`)

fn main() -> felt {
    let is_morning = true;
    if is_morning {
        return 1;
    }

    // TODO: Define a boolean variable with the name `is_evening` before the `if` statement below.
    // The value of the variable should be the negation (opposite) of `is_morning`.
    let is_evening = !is_morning;
    if is_evening {
        return 2;
    }

    return 0;
}
